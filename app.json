{
  "expo": {
    "name": "forthtek-app",
    "slug": "forthtek-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ForthTek.png",
    "splash": {
      "image": "./assets/ForthTek.png",
      "resizeMode": "contain",
      "backgroundColor": "#00a8f3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ForthTek.png",
        "backgroundColor": "#00a8f3"
      },
      "package": "app.pictohunt"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
